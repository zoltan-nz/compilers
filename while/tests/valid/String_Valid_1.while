bool has(char c, string str) {
    int i = 0;
    while(i < |str|) {
        if(str[i] == c) {
            return true;
	} 
        i = i + 1;
    }
    return false;
}

void main() {
    string s = "Hello World";
    print has('l',s);
    print has('e',s);
    print has('h',s);
    print has('z',s);
    print has('H',s);
}
