bool has(char c1, string str) {
    for(int i=0;i!=|str|;i=i+1) {
        if(c1 == str[i]) {
            return true;
	}
    }
    return false;
}

void main() {
    string s = "Hello World";
    print has('l',s);
    print has('e',s);
    print has('h',s);
    print has('z',s);
    print has('H',s);
}
