type pos is real
type neg is int
type expr is pos|neg|[int]

string f(expr e) {
    if(e is real && ((real)e) > 0.0) {
        return "POSITIVE: " ++ e;
    } else if(e is neg) {
        return "NEGATIVE: " ++ e;
    } else {
        return "OTHER";
    }
}

void main() {
    print f(-1);
    print f(1.0);
    print f(1.234);
    print f([1,2,3]);
}
 
